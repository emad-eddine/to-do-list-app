{
  "expo": {
    "name": "to-do-list",
    "slug": "to-do-list",
    "version": "1.0.0",
    "scheme": "my-app-scheme" ,
    "orientation": "portrait",
    "icon": "./app/assets/icons/app_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/icons/app_icon.png",
      "resizeMode": "native",
      "backgroundColor": "#EDF4F2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icons/app_icon.png",
        "backgroundColor": "#EDF4F2"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
